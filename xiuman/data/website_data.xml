<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">
    <record id="website.default_website" model="website">
        <field name="name">Xiuman Agro</field>
        <field name="prevent_zero_price_sale" eval="True" />
        <field name="show_line_subtotals_tax_selection">tax_included</field>
        <field name="default_lang_id" ref="base.lang_es_MX" />
        <field
            name="language_ids"
            eval="[
                Command.link(ref('base.lang_en')),
                Command.link(ref('base.lang_es_MX')),
            ]"
        />
    </record>
</odoo>
