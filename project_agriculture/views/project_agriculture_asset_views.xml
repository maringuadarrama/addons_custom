<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_project_agriculture_asset_search" model="ir.ui.view">
        <field name="name">project.agriculture.asset.search</field>
        <field name="model">project.agriculture.asset</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" />
            </search>
        </field>
    </record>

    <record id="view_project_agriculture_asset_tree" model="ir.ui.view">
        <field name="name">project.agriculture.asset.tree</field>
        <field name="model">project.agriculture.asset</field>
        <field name="arch" type="xml">
            <tree string="Lands">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="view_project_agriculture_asset_form" model="ir.ui.view">
        <field name="name">project.agriculture.asset.form</field>
        <field name="model">project.agriculture.asset</field>
        <field name="arch" type="xml">
            <form string="Land">
                <sheet>
                    <group>
                        <group>
                            <field name="name" />
                        </group>
                        <group>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
