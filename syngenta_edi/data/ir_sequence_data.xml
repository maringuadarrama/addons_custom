<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

    <record id="seq_syngenta_sale_document" model="ir.sequence">
        <field name="name">Syngenta Sale Document</field>
        <field name="code">syngenta.sale.document</field>
        <field name="prefix">SALE</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="seq_syngenta_stock_document" model="ir.sequence">
        <field name="name">Syngenta Stock Document</field>
        <field name="code">syngenta.stock.document</field>
        <field name="prefix">STOCK</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>

</odoo>
