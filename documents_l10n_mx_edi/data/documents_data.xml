<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">

    <!-- Partner Tags -->
    <record id="documents_edi_automatic_partner_tag" model="res.partner.category">
        <field name="name">EDI Automatic</field>
    </record>

    <record id="documents_edi_partner_requires_po_tag" model="res.partner.category">
        <field name="name">EDI Requires PO</field>
    </record>

    <!-- Folders -->
    <record id="l10n_edi_document_folder_edi_doc" model="documents.folder" forcecreate="0">
        <field name="name">EDI Documents</field>
        <field name="sequence">12</field>
    </record>

    <record id="l10n_edi_document_folder_edi_received" model="documents.folder" forcecreate="0">
        <field name="name">Received</field>
        <field name="parent_folder_id" ref="l10n_edi_document_folder_edi_doc" />
        <field name="sequence">13</field>
    </record>

    <record id="l10n_edi_document_folder_edi_issued" model="documents.folder" forcecreate="0">
        <field name="name">Issued</field>
        <field name="parent_folder_id" ref="l10n_edi_document_folder_edi_doc" />
        <field name="sequence">14</field>
    </record>

    <!-- Facet -->
    <record id="l10n_mx_edi_documents_facet_processed" model="documents.facet">
        <field name="name">Processed</field>
        <field name="folder_id" ref="l10n_edi_document_folder_edi_doc" />
        <field name="sequence">10</field>
    </record>

    <record id="xunnel_xml_facet" model="documents.facet">
        <field name="name">CFDI type</field>
        <field name="folder_id" ref="l10n_edi_document_folder_edi_doc" />
        <field name="sequence">11</field>
    </record>

    <record id="l10n_edi_document_facet_fiscal_year" model="documents.facet">
        <field name="name">Fiscal year</field>
        <field name="folder_id" ref="l10n_edi_document_folder_edi_doc" />
        <field name="sequence">12</field>
    </record>

    <record id="l10n_edi_document_facet_fiscal_month" model="documents.facet">
        <field name="name">Fiscal month</field>
        <field name="folder_id" ref="l10n_edi_document_folder_edi_doc" />
        <field name="sequence">13</field>
    </record>

    <!-- Tags -->
    <record id="l10n_edi_document_to_process" model="documents.tag" forcecreate="0">
        <field name="name">To Process</field>
        <field name="facet_id" ref="l10n_mx_edi_documents_facet_processed" />
        <field name="sequence">11</field>
    </record>

    <record id="l10n_edi_document_processed" model="documents.tag" forcecreate="0">
        <field name="name">Processed</field>
        <field name="facet_id" ref="l10n_mx_edi_documents_facet_processed" />
        <field name="sequence">10</field>
    </record>

    <record id="ingreso_tag" model="documents.tag">
        <field name="name">Income</field>
        <field name="facet_id" ref="xunnel_xml_facet" />
        <field name="sequence">1</field>
    </record>

    <record id="egreso_tag" model="documents.tag">
        <field name="name">Egress</field>
        <field name="facet_id" ref="xunnel_xml_facet" />
        <field name="sequence">2</field>
