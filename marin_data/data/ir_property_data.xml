    <record id="default_account_payable_main_company" model="ir.property" forcecreate="True">
        <field name="name">property_account_payable_id</field>
        <field name="company_id" ref="base.main_company" />
        <field
            name="fields_id"
            search="[('model', '=', 'res.partner'), ('name', '=', 'property_account_payable_id')]"
        />
        <field name="value" eval="'account.account,'+ str(ref('account_201_01_001_main_company'))" />
    </record>

