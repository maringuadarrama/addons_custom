<?xml version="1.0" encoding="UTF-8" ?>
<templates>
    <t t-inherit="documents.FileViewer" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('o-FileViewer-main')]" position="inside">
            <iframe
                t-if="state.file.isMxXml"
                class="o-FileViewer-view o_AttachmentViewer_isMxXml o_text w-75 h-100 border-0"
                t-att-src="state.file.defaultSource"
            />
        </xpath>
    </t>
</templates>
